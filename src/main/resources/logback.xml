<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %logger{10} %-5level %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="net.dv8tion.jda" level="INFO" />

    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
